{
  "name": "rules-catalog",
  "version": "1.0.0",
  "description": "rules catalog",
  "main": "server.js",
  "scripts": {
    "build": "webpack -w",
    "lint": "xo ./server.js ./source/js/*.jsx",
    "jscomplex": "jscomplex \"['./source/js/**/*.jsx', './*.js']\""
  },
  "author": "jansanchez",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "express": "^4.13.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "jscomplex": "^1.0.2",
    "webpack": "^1.13.0",
    "xo": "^0.14.0"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "babel/new-cap": 0,
      "no-undef": 0,
      "indent": 0,
      "no-unused-vars": 0
    }
  }
}
